{
    "staging": {
        "app_function": "manage.application",
        "apigateway_enabled": false,
        "slim_handler": true,
        "aws_region": "us-east-1",
        "profile_name": "zapcore",
        "project_name": "zapcore_app",
        "runtime": "python3.6",
        "s3_bucket": "zapcore",
        "environment_variables": {
            "FLASK_SETTINGS_MODULE": "server.config.StagingConfig"
        },
        "iam_authorization": true,
        "delete_zip": true,
        "cors": true,
        "exclude": [".bin/*", ".circle/*", "./config", "client/*", "dist/*", "*.log", "*.gz", "*.rar"]
    },
    "staging_ap_northeast_1": {
        "aws_region": "ap-northeast-1",
        "extends": "staging"
    },
    "staging_ap_south_1": {
        "aws_region": "ap-south-1",
        "extends": "staging"
    },
    "staging_ap_southeast_1": {
        "aws_region": "ap-southeast-1",
        "extends": "staging"
    },
    "staging_eu_central_1": {
        "aws_region": "eu-central-1",
        "extends": "staging"
    },
    "staging_eu_west_1": {
        "aws_region": "eu-west-1",
        "extends": "staging"
    },
    "staging_us_west_1": {
        "aws_region": "us-west-1",
        "extends": "staging"
    }
}